<mvc:View controllerName="sap.btp.ui5.controller.List" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
    <Page id="listPage" title="{i18n>ListTitle}">
        <List id="list" items="{Products}">
            <StandardListItem id="standardlistitem" type="Navigation" press="handleListItemPress" title="{ProductName}"/>
        </List>
    </Page>
</mvc:View>
